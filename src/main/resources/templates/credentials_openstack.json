{
   "cloudPlatform": "$CLOUD_PLATFORM",
   "name": "$NAME",
   "description": "$DESCRIPTION",
   "publicKey": "$SSHKEY",
   "parameters":{
      "userName": "$USERNAME",
      "password": "$PASSWORD",
      "endpoint": "$ENDPOINT",
      "keystoneVersion": "$KEYSTONE_VERSION",
      "selector": "$SELECTOR",
      "tenantName": ${TENANTNAME == null ? 'null' : "\"$TENANTNAME\""},
      "userDomain": ${USERDOMAIN == null ? 'null' : "\"$USERDOMAIN\""},
      "keystoneAuthScope": ${KEYSTONEAUTHSCOPE == null ? 'null' : "\"$KEYSTONEAUTHSCOPE\""},
      "domainName": ${DOMAINNAME == null ? 'null' : "\"$DOMAINNAME\""},
      "projectDomainName": ${PROJECTDOMAINNAME == null ? 'null' : "\"$PROJECTDOMAINNAME\""},
      "projectName": ${PROJECTNAME == null ? 'null' : "\"$PROJECTNAME\""}
   }
}